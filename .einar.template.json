            "kind":"get-controller",
            "adapter_type":"inbound",
            "command":"einar generate get-controller ${operation_name}",
            "depends_on":[""],
            "files": [{
                "source_file": "app/adapter/in/api/get.go",
                "destination_dir": "app/adapter/in/api",
                "ioc_discovery":true,
                "replace_holders": [
                    {
                        "kind":"camelCase",
                        "name": "newTemplateGet",
                        "append_at_start":"",
                        "append_at_end":""
                    }
                ]
            },
            {
                "source_file": "app/adapter/in/api/get_test.go",
                "destination_dir": "app/adapter/in/api",
                "ioc_discovery":true,
                "append_at_end":"_test",
                "replace_holders": [
                    {
                        "kind":"PascalCase",
                        "name": "TestNewTemplateGet",
                        "append_at_start":"Test",
                        "append_at_end":""
                    },
                    {
                        "kind":"camelCase",
                        "name": "newTemplateGet",
                        "append_at_start":"",
                        "append_at_end":""
                    }
