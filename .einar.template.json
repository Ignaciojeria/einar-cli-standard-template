{
    "installations_base": [
        {
            "name":"godotenv",
            "library": "github.com/joho/godotenv"
        },
        {
            "name":"einar-ioc",
            "library": "github.com/Ignaciojeria/einar-ioc"
        },
        {
            "name":"echo",
            "library": "github.com/labstack/echo/v4"
        },
        {
            "name":"otel",
            "library": "go.opentelemetry.io/otel"
        },
        {
            "name":"otlptrace",
            "library": "go.opentelemetry.io/otel/exporters/otlp/otlptrace"
        },
        {
            "name":"otlptracegrpc",
            "library": "go.opentelemetry.io/otel/exporters/otlp/otlptrace/otlptracegrpc"
        },
        {
            "name":"uuid",
            "library": "github.com/google/uuid"
        }
    ],
    "base_template": {
       "folders": [
            {
                "source_dir": "app/configuration",
                "destination_dir": "app/configuration"
            },
            {
                "source_dir": "app/infrastructure/serverwrapper",
                "destination_dir": "app/infrastructure/serverwrapper"
            },
            {
                "source_dir": "app/adapter/out/slog",
                "destination_dir": "app/adapter/out/slog"
            },
            {
                "source_dir": "app/exception",
                "destination_dir": "app/exception"
            },
            {
                "source_dir": "app/constants",
                "destination_dir": "app/constants"
            },
            {
                "source_dir": "app/infrastructure/observability",
                "destination_dir": "app/infrastructure/observability"
            }
        ],
        "files": [
            {
                "source_file": "main.go",
                "destination_file": "main.go"
            },
            {
                "source_file": ".env",
                "destination_file": ".env"
            },
            {
                "source_file": ".gitignore",
                "destination_file": ".gitignore"
            }
        ]
    },    
    "component_commands":[
